package main

import (
	"fmt"
)

// there is a difference between the fmt.Print and fmt.Println functions. The later adds a new line after printing each character.
func reverse(s string) {
	for i := len(s) - 1; i >= 0; i-- {
		fmt.Print(string(s[i]))
	}
	//fmt.Println()
	fmt.Print("\n")
}

func main() {
	text := "master"
	reverse(text)
}